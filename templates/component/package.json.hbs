{
  "name": "@destyler/{{machine}}",
  "type": "module",
  "version": "0.0.0",
  "author": "Elone Hoo <elonehoo@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "clear:node_modules": "rimraf ./node_modules",
    "prepublishOnly": "pnpm run build",
    "build": "pnpm run clear && tsdown",
    "watch": "tsdown --watch"
  },
  "dependencies": {
    "@destyler/anatomy": "workspace:*",
    "@destyler/core": "workspace:*",
    "@destyler/dom": "workspace:*",
    "@destyler/types": "workspace:*",
    "@destyler/utils": "workspace:*"
  }
}
