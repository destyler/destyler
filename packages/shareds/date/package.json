{
  "name": "@destyler/date",
  "type": "module",
  "version": "0.1.0",
  "description": "Date utilities for web",
  "author": "Elone Hoo <elonehoo@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "clear:node_modules": "rimraf ./node_modules",
    "prepublishOnly": "pnpm run build",
    "build": "pnpm run clear && tsdown",
    "watch": "tsdown --watch"
  },
  "peerDependencies": {
    "@internationalized/date": ">=3.0.0"
  },
  "devDependencies": {
    "@internationalized/date": "catalog:prod"
  }
}
