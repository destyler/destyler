{
  "name": "@destyler/react",
  "type": "module",
  "version": "0.0.7-beta.9",
  "description": "the react wrapper for destyler",
  "author": "Elone Hoo <elonehoo@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "clear:node_modules": "rimraf ./node_modules",
    "prepublishOnly": "pnpm run build",
    "build": "pnpm run clear && tsdown",
    "watch": "tsdown --watch"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@destyler/store": "workspace:*",
    "@destyler/types": "workspace:*",
    "@destyler/xstate": "workspace:*",
    "proxy-compare": "catalog:prod"
  },
  "devDependencies": {
    "@destyler/utils": "workspace:*",
    "react": "catalog:frontend",
    "react-dom": "catalog:frontend"
  }
}
