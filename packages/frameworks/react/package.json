{
  "name": "@destyler/react",
  "type": "module",
  "version": "0.0.7-beta.2",
  "description": "the react wrapper for destyler",
  "author": "Elone Hoo <elonehoo@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "clear:node_modules": "rimraf ./node_modules",
    "prepublishOnly": "pnpm run build",
    "build": "pnpm run clear && unbuild",
    "stub": "unbuild --stub"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@zag-js/core": "catalog:",
    "@zag-js/store": "catalog:",
    "@zag-js/types": "catalog:",
    "proxy-compare": "catalog:"
  },
  "devDependencies": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@zag-js/utils": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:"
  }
}
