{
  "name": "@destyler/range-calendar",
  "type": "module",
  "version": "0.0.2",
  "description": "unstyled range calendar component for vue",
  "author": "Elone Hoo <elonehoo@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./resolver": {
      "types": "./dist/resolver.d.ts",
      "import": "./dist/resolver.mjs",
      "require": "./dist/resolver.cjs"
    },
    "./component": {
      "types": "./dist/component.d.ts",
      "import": "./dist/component.mjs",
      "require": "./dist/component.cjs"
    },
    "./composition": {
      "types": "./dist/composition.d.ts",
      "import": "./dist/composition.mjs",
      "require": "./dist/composition.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "prepublishOnly": "pnpm run build",
    "build": "pnpm run clear && unbuild"
  },
  "peerDependencies": {
    "vue": ">=3.3.4"
  },
  "dependencies": {
    "@destyler/calendar": "workspace:*",
    "@destyler/composition": "workspace:*",
    "@destyler/primitive": "workspace:*",
    "@destyler/shared": "workspace:*",
    "@internationalized/date": "^3.5.2"
  }
}
