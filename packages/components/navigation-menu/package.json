{
  "name": "@destyler/navigation-menu",
  "type": "module",
  "version": "0.2.6-beta.1",
  "author": "Elone Hoo <elonehoo@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/destyler/destyler",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/destyler/destyler.git",
    "directory": "packages/components/navigation-menu"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "clear:node_modules": "rimraf ./node_modules",
    "prepublishOnly": "pnpm run build",
    "build": "pnpm run clear && tsdown",
    "watch": "tsdown --watch"
  },
  "dependencies": {
    "@destyler/anatomy": "workspace:*",
    "@destyler/dom": "workspace:*",
    "@destyler/types": "workspace:*",
    "@destyler/utils": "workspace:*",
    "@destyler/xstate": "workspace:*"
  }
