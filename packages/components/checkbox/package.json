{
  "name": "@destyler/checkbox",
  "type": "module",
  "version": "0.0.7-beta.7",
  "description": "a control that allows the user to toggle between checked and not checked.",
  "author": "Elone Hoo <elonehoo@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "clear:node_modules": "rimraf ./node_modules",
    "prepublishOnly": "pnpm run build",
    "build": "pnpm run clear && unbuild",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "@destyler/anatomy": "workspace:*",
    "@destyler/dom": "workspace:*",
    "@destyler/focus-visible": "workspace:*",
    "@destyler/types": "workspace:*",
    "@destyler/utils": "workspace:*",
    "@destyler/xstate": "workspace:*"
  }
}
