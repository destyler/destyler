{
  "name": "@destyler/avatar",
  "version": "0.0.0",
  "description": "unstyled avatar component for vue",
  "author": "Elone Hoo <elonehoo@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "dev": "vite build",
    "build:type": "vue-tsc --declaration --emitDeclarationOnly",
    "prepublishOnly": "pnpm run build",
    "build": "pnpm run clear && pnpm run dev && pnpm run build:type"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  }
}
