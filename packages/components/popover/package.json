{
  "name": "@destyler/popover",
  "version": "0.0.0",
  "description": "unstyled popover component for vue",
  "author": "Elone Hoo <elonehoo@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "build": "pnpm run clear && unbuild"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "dependencies": {
    "@destyler/binder": "workspace:*",
    "@destyler/composition": "workspace:*",
    "@destyler/directives": "workspace:*",
    "@destyler/focus-trap": "workspace:^",
    "@destyler/follower": "workspace:*",
    "@destyler/shared": "workspace:*"
  }
}
