{
  "name": "@destyler/calendar",
  "type": "module",
  "version": "0.0.7-beta.7",
  "description": "displays dates and days of the week, facilitating date-related interactions.",
  "author": "Elone Hoo <elonehoo@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "clear:node_modules": "rimraf ./node_modules",
    "prepublishOnly": "pnpm run build",
    "build": "pnpm run clear && tsdown",
    "watch": "tsdown --watch"
  },
  "peerDependencies": {
    "@internationalized/date": ">=3.0.0"
  },
  "dependencies": {
    "@destyler/anatomy": "workspace:*",
    "@destyler/date": "workspace:*",
    "@destyler/dismissable": "workspace:*",
    "@destyler/dom": "workspace:*",
    "@destyler/live-region": "workspace:*",
    "@destyler/popper": "workspace:*",
    "@destyler/types": "workspace:*",
    "@destyler/utils": "workspace:*",
    "@destyler/xstate": "workspace:*"
  },
  "devDependencies": {
    "@internationalized/date": "catalog:prod"
  }
}
