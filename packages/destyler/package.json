{
  "name": "destyler",
  "version": "0.0.1-beta.9",
  "description": "",
  "author": "Elone Hoo <elonehoo@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./resolver": {
      "types": "./dist/resolver.d.ts",
      "import": "./dist/resolver.mjs",
      "require": "./dist/resolver.cjs"
    },
    "./presets": {
      "types": "./dist/presets.d.ts",
      "import": "./dist/presets.mjs",
      "require": "./dist/presets.cjs"
    },
    "./component": {
      "types": "./dist/component.d.ts",
      "import": "./dist/component.mjs",
      "require": "./dist/component.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "prepublishOnly": "pnpm run build",
    "build": "pnpm run clear && unbuild"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "dependencies": {
    "@destyler/arrow": "workspace:*",
    "@destyler/aspect-radio": "workspace:*",
    "@destyler/avatar": "workspace:*",
    "@destyler/back-top": "workspace:*",
    "@destyler/button": "workspace:*",
    "@destyler/checkbox": "workspace:*",
    "@destyler/collapse": "workspace:*",
    "@destyler/collapsible": "workspace:*",
    "@destyler/countdown": "workspace:*",
    "@destyler/dialog": "workspace:*",
    "@destyler/dismissable-layer": "workspace:*",
    "@destyler/focus-scope": "workspace:*",
    "@destyler/icon": "workspace:*",
    "@destyler/info": "workspace:*",
    "@destyler/label": "workspace:*",
    "@destyler/modal": "workspace:*",
    "@destyler/online": "workspace:*",
    "@destyler/popover": "workspace:*",
    "@destyler/popper": "workspace:*",
    "@destyler/presence": "workspace:*",
    "@destyler/preview": "workspace:*",
    "@destyler/primitive": "workspace:*",
    "@destyler/progress": "workspace:*",
    "@destyler/select": "workspace:*",
    "@destyler/slider": "workspace:*",
    "@destyler/teleport": "workspace:*",
    "@destyler/tooltip": "workspace:*",
    "@destyler/visually-hidden": "workspace:*"
  }
}
