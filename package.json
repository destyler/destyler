{
  "name": "@destyler/monorepo",
  "type": "module",
  "version": "0.0.7-beta.8",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "author": "Elone Hoo <elonehoo@gmail.com>",
  "license": "MIT",
  "keywords": [
    "web component",
    "unstyle"
  ],
  "scripts": {
    "build": "pnpm run --filter \"./packages/**\" -r build",
    "watch": "pnpm run --filter \"./packages/**\" -r watch",
    "clear:node_modules": "rm -rf ./node_modules && rm -rf pnpm-lock.yaml",
    "clear:packages:dist": "pnpm run --filter \"./packages/**\" -r clear ",
    "clear:packages:node_modules": "pnpm run --filter \"./packages/**\" -r clear:node_modules",
    "clear": "pnpm run clear:packages:dist && pnpm run clear:packages:node_modules && pnpm run clear:node_modules",
    "generate-machine": "plop machine",
    "generate-example": "plop examples",
    "generate-shared": "plop shared",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "taze": "taze",
    "test": "vitest run --browser.headless",
    "test:dev": "vitest dev",
    "test:ui": "vitest --ui --open",
    "test:deploy": "vitest run --reporter=html --browser.headless",
    "vanilla": "pnpm --filter \"./examples/vanilla\"",
    "vue": "pnpm --filter \"./examples/vue\"",
    "react": "pnpm --filter \"./examples/react\"",
    "solid": "pnpm --filter \"./examples/solid\"",
    "svelte": "pnpm --filter \"./examples/svelte\"",
    "node-modules:dev": "node-modules-inspector",
    "node-modules:build": "node-modules-inspector build",
    "release": "bumpp package.json packages/**/*/package.json --commit --push --tag"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:lint",
    "@destyler/dom": "workspace:*",
    "@destyler/presence": "workspace:*",
    "@destyler/react": "workspace:*",
    "@destyler/shared-private": "workspace:*",
    "@destyler/solid": "workspace:*",
    "@destyler/stringify": "workspace:*",
    "@destyler/svelte": "workspace:*",
    "@destyler/tree": "workspace:*",
    "@destyler/vanilla": "workspace:*",
    "@destyler/vue": "workspace:*",
    "@sveltejs/vite-plugin-svelte": "catalog:plugin",
    "@types/node": "catalog:types",
    "@types/react": "catalog:types",
    "@types/react-dom": "catalog:types",
    "@unocss/reset": "catalog:css",
    "@vitejs/plugin-react": "catalog:plugin",
    "@vitejs/plugin-vue": "catalog:plugin",
    "@vitest/browser": "catalog:test",
    "@vitest/eslint-plugin": "catalog:test",
    "@vitest/ui": "catalog:test",
    "bumpp": "catalog:script",
    "cross-env": "catalog:util",
    "eslint": "catalog:lint",
    "nanoid": "catalog:util",
    "node-modules-inspector": "catalog:visual",
    "playwright": "catalog:test",
    "plop": "catalog:script",
    "react": "catalog:frontend",
    "react-dom": "catalog:frontend",
    "rimraf": "catalog:script",
    "solid-js": "catalog:frontend",
    "svelte": "catalog:frontend",
    "taze": "catalog:script",
    "tsdown": "catalog:build",
    "tsx": "catalog:script",
    "typescript": "catalog:types",
    "unocss": "catalog:css",
    "vite-plugin-solid": "catalog:plugin",
    "vitest": "catalog:test",
    "vitest-axe": "catalog:test",
    "vitest-browser-react": "catalog:test",
    "vitest-browser-solid": "catalog:test",
    "vitest-browser-svelte": "catalog:test",
    "vitest-browser-vue": "catalog:test",
    "vue": "catalog:frontend"
  }
}
